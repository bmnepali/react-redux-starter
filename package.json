{
  "name": "react-redux-starter",
  "version": "1.0.0",
  "description": "React Redux Boilerplate with linters and preetifier :)",
  "main": "index.js",
  "keywords": [
    "react",
    "redux",
    "scss",
    "webpack",
    "jest",
    "enzyme",
    "stylelint",
    "eslint",
    "preetier"
  ],
  "author": "Buddha Man Nepali",
  "maintainers": [
    "Buddha Man Nepali"
  ],
  "repository": "https://github.com/bmnepali/react-redux-starter.git",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --config ./webpack.config.js --mode development",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/.bin/jest --runInBand --watch",
    "test:coverage": "jest --coverage --colors",
    "lint:style": "stylelint ./src/styles/**/*.scss ./src/styles/**/*.css",
    "lint:style:fix": "stylelint ./src/styles/**/*.css ./src/styles/**/*.css --fix",
    "lint:js": "eslint ./src",
    "lint:js:fix": "eslint ./src --fix",
    "pretify": "prettier --write ./src/**/*.js"
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "redux": "^4.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^6.1.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-mocha": "^6.0.0",
    "eslint-plugin-react": "^7.14.3",
    "husky": "^3.0.1",
    "jest": "^24.8.0",
    "jest-cli": "^24.8.0",
    "lint-staged": "^9.2.1",
    "prettier": "^1.18.2",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-order": "^3.0.1",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(js|jsx)": [
      "npm run pretify",
      "npm run lint:js:fix",
      "npm run lint:style:fix",
      "git add"
    ]
  }
}
